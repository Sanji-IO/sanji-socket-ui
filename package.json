{
  "name": "sanji-socket-ui",
  "author": "Zack Yang <zack9433@gmail.com> (https://github.com/zack9433)",
  "description": "sanji-exception-ui UI",
  "main": "index",
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:once && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "NODE_ENV=production webpack --config webpack.build.js --display-modules",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "NODE_ENV=development node server/dev-server.js",
    "test": "mocha -w -R spec --compilers js:babel/register ./app/component/*.spec.js",
    "test:once": "istanbul cover -x *.spec.js _mocha -- -R spec --compilers js:babel-core/register ./app/component/*.spec.js",
    "publish-latest": "publish-latest",
    "semantic-release": "semantic-release pre && npm publish && npm run publish-latest && semantic-release post"
  },
  "devDependencies": {
    "angular-mocks": "~1.5.0",
    "babel-core": "~5.8.34",
    "babel-eslint": "~4.1.6",
    "babel-loader": "~5.3.3",
    "babel-runtime": "~5.8.34",
    "benv": "~2.1.0",
    "chai": "~3.5.0",
    "codecov.io": "~0.1.6",
    "commitizen": "~2.7.0",
    "cz-conventional-changelog": "~1.1.5",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.2.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "ghooks": "~1.0.3",
    "html-webpack-plugin": "~1.7.0",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5",
    "ng-annotate-loader": "~0.1.0",
    "node-libs-browser": "~1.0.0",
    "publish-latest": "~1.1.2",
    "semantic-release": "^4.3.5",
    "socket.io": "~1.4.4",
    "webpack": "~1.12.10",
    "webpack-dev-server": "~1.14.0",
    "webpack-notifier": "~1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanji-IO/sanji-socket-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Sanji-IO/sanji-socket-ui/issues"
  },
  "keywords": [
    "sanji"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Sanji-IO/sanji-socket-ui",
  "dependencies": {
    "angular": "~1.5.0",
    "angular-socket-io": "~0.7.0",
    "socket.io-client": "~1.4.4"
  }
}
