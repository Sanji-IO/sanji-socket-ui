{
  "name": "sanji-socket-ui",
  "description": "sanji-exception-ui UI",
  "main": "index",
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:once && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "NODE_ENV=production webpack --config webpack.build.js --display-modules",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "NODE_ENV=development node server/dev-server.js",
    "test": "mocha -w -R spec --compilers js:babel/register ./app/component/*.spec.js",
    "test:once": "istanbul cover -x *.spec.js _mocha -- -R spec --compilers js:babel/register ./app/component/*.spec.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "angular-material": "~1.0.1",
    "angular-animate": "~1.4.7",
    "angular-aria": "~1.4.7",
    "angular-material-icons": "~0.6.0",
    "angular-mocks": "~1.4.7",
    "angular-sanji-window": "Sanji-IO/angular-sanji-window#webpack-sanji-window",
    "autoprefixer-loader": "~3.1.0",
    "babel": "~5.8.34",
    "babel-core": "~5.8.34",
    "babel-eslint": "~4.1.6",
    "babel-loader": "~5.3.3",
    "babel-runtime": "~5.8.34",
    "benv": "~2.1.0",
    "chai": "~3.4.1",
    "codecov.io": "~0.1.6",
    "commitizen": "~2.5.0",
    "css-loader": "~0.23.1",
    "cz-conventional-changelog": "~1.1.5",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.2.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "ghooks": "~1.0.3",
    "html-webpack-plugin": "~1.7.0",
    "istanbul": "~0.4.2",
    "jquery": "~2.2.0",
    "mocha": "~2.3.4",
    "ng-annotate-loader": "0.0.10",
    "ng-cache-loader": "0.0.15",
    "node-bourbon": "~4.2.3",
    "node-libs-browser": "~0.5.2",
    "node-sass": "~3.4.2",
    "raw-loader": "~0.5.1",
    "sass-loader": "~3.1.2",
    "semantic-release": "~4.3.5",
    "socket.io": "~1.4.4",
    "style-loader": "~0.12.3",
    "svg-morpheus": "alexk111/SVG-Morpheus#v0.1.8",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.10",
    "webpack-dev-server": "~1.14.0",
    "webpack-notifier": "~1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanji-IO/sanji-socket-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Sanji-IO/sanji-socket-ui/issues"
  },
  "keywords": [
    "sanji"
  ],
  "author": {
    "name": "Zack Yang",
    "email": "zack9433@gmail.com",
    "url": "https://github.com/Sanji-IO"
  },
  "license": "MIT",
  "homepage": "https://github.com/Sanji-IO/sanji-socket-ui",
  "dependencies": {
    "angular": "~1.4.7",
    "angular-socket-io": "~0.7.0",
    "socket.io-client": "~1.4.4"
  }
}
